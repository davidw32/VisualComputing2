<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="elementBar" alignment="TOP_CENTER" maxHeight="820.0" minHeight="820.0" minWidth="100.0" spacing="50.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ElementBarController">

    <children>
        <Label fx:id="elementBarTitle" text="Preview" />

        <VBox alignment="CENTER" prefHeight="100">
            <Circle fx:id="ballDummy" fill="PLUM" radius="40.0" />
        </VBox>
        <VBox alignment="CENTER" prefHeight="100">
            <Rectangle fx:id="blockDummy" fill="SEAGREEN" height="20.0" rotate="-20.0" width="90.0" />
        </VBox>
        <VBox alignment="CENTER" prefHeight="100">
      <Text fx:id="springboardDummy" strokeType="OUTSIDE" strokeWidth="0.0" text="Springboard">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
        </VBox>
        <VBox alignment="CENTER" prefHeight="100" fx:id="spinnerDummy">
         <Group >
            <children>
               <Rectangle fill="ORANGERED" height="10.0"  width="90.0" />
               <Circle centerX="45.0" centerY="5.0" radius="3.0" />
            </children>
         </Group>
        </VBox>
        <VBox alignment="CENTER" prefHeight="100" fx:id="seesawDummy">
         <Group >
            <children>
               <Rectangle fill="DODGERBLUE" height="10.0" rotate="14.0"  width="90.0" />
               <Polygon fill="DODGERBLUE" layoutX="45.0" layoutY="9.0" points="-10.0, 13.0, 10.0, 13, 0, 0"  />
            </children>
         </Group>
        </VBox>


    </children>

    <padding>
        <Insets top="5.0" />
    </padding>
</VBox>
